<?xml version="1.0" encoding="utf-8"?>
<!-- SIGN.xml sample.
     Please change values and
     rename SIGN.sample.xml to SIGN.xml
     You also can add some new parameters
     (use `jarsigner -help' for extra info).
-->
<signature>
    <key>YourKeyName</key>
    <parameters>
        <param name="sigalg" value="SHA1withRSA" />
        <param name="digestalg" value="SHA1" />
        <param name="keystore" value="/path/to/YourKeyStorage.keystore" />
        <param name="storepass" value="YourStoragePassword" />
        <param name="keypass" value="YourKeyPassword" />
    </parameters>
</signature>
